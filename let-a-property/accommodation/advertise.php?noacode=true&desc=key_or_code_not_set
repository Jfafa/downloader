    function submitForm()
    {
        if( frmAddLandlord.selConditions[ frmAddLandlord.selConditions.selectedIndex ].value == 0 )
        {
            alert( "Please agree to the terms and conditions." );
            frmAddLandlord.selConditions.focus();
        }
        else
        {
            if( frmAddLandlord.selTitle[ frmAddLandlord.selTitle.selectedIndex ].value == "null" )
            {
                alert( "Please select a title" );
                frmAddLandlord.selTitle.focus();
            }
            else
            {
                if( frmAddLandlord.txtGivenName.value == "" )
                {
                    alert( "Please enter a given name" );
                    frmAddLandlord.txtGivenName.focus();
                }
                else
                {
                    if( frmAddLandlord.txtSurname.value == "" )
                    {
                        alert( "Please enter a surname" );
                        frmAddLandlord.txtSurname.focus();
                    }
                    else
                    {
                        if( frmAddLandlord.selLandlordType[ frmAddLandlord.selLandlordType.selectedIndex ].value == "null" )
                        {
                            alert( "Please select a landlord type" );
                            frmAddLandlord.selLandlordType.focus();
                        }
                        else
                        {
                            if( frmAddLandlord.txtAddress.value == "" )
                            {
                                alert( "Please enter an address" );
                                frmAddLandlord.txtAddress.focus();
                            }
                            else
                            {
                                if( frmAddLandlord.txtTown.value == "" )
                                {
                                    alert( "Please enter a town" );
                                    frmAddLandlord.txtTown.focus();
                                }
                                else
                                {
                                    if( frmAddLandlord.selCounties[ frmAddLandlord.selCounties.selectedIndex ].value == "null" )
                                    {
                                        alert( "Please select a county" );
                                        frmAddLandlord.selCounties.focus();
                                    }
                                    else
                                    {
                                        if( frmAddLandlord.txtPostCode.value == "" )
                                        {
                                            alert( "Please enter a postcode" );
                                            frmAddLandlord.txtPostCode.focus();
                                        }
                                        else
                                        {
                                            if( frmAddLandlord.txtEmailAddress.value == "" )
                                            {
                                                alert( "Please enter an email address." );
                                                frmAddLandlord.txtEmailAddress.focus();
                                            }
                                            else
                                            {
                                                if( frmAddLandlord.txtUsername.value == "" )
                                                {
                                                    alert( "Please enter a username" );
                                                    frmAddLandlord.txtUsername.focus();
                                                }
                                                else
                                                {
                                                    if( frmAddLandlord.txtPassword.value == "" )
                                                    {
                                                        alert( "Please enter a password" );
                                                        frmAddLandlord.txtPassword.focus();
                                                    }
                                                    else
                                                    {
                                                        if( frmAddLandlord.txtPassword.value != frmAddLandlord.txtPassword2.value )
                                                        {
                                                            alert( "Your passwords do not match, please re-type your passwords." );
                                                            frmAddLandlord.txtPassword.value = "";
                                                            frmAddLandlord.txtPassword2.value = "";
                                                            frmAddLandlord.txtPassword.focus();
                                                        }
                                                        else
                                                        {
                                                            frmAddLandlord.submit();
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }

    function popUp(URL)
    {
    day = new Date();
    id = day.getTime();
